<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
     xmlns:app="http://schemas.android.com/apk/res-auto"
     xmlns:tools="http://schemas.android.com/tools"
     android:layout_width="match_parent"
     android:layout_height="match_parent"
     android:accessibilityLiveRegion="assertive"
     tools:context=".Activities.AccesCount.AccesCount">

    <ImageButton
         android:id="@+id/botonRegistro"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_marginStart="12dp"
         android:layout_marginTop="440dp"
         android:layout_marginEnd="12dp"
         android:alpha="23"
         android:background="#00000000"
         android:src="@drawable/ic_boton_registro"

         app:layout_constraintEnd_toEndOf="parent"
         app:layout_constraintHorizontal_bias="0.492"
         app:layout_constraintStart_toStartOf="parent"
         app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
         android:id="@+id/buttonLogear"
         android:layout_width="356dp"
         android:layout_height="71dp"
         android:layout_marginStart="16dp"
         android:layout_marginTop="12dp"
         android:layout_marginEnd="16dp"
         android:layout_marginBottom="104dp"
         android:background="#00000000"
         android:src="@drawable/ic_boton_login"
         app:layout_constraintBottom_toBottomOf="parent"
         app:layout_constraintEnd_toEndOf="parent"
         app:layout_constraintStart_toStartOf="parent"
         app:layout_constraintTop_toBottomOf="@+id/botonRegistro"
         app:layout_constraintVertical_bias="0.067" />

    <ImageView
         android:id="@+id/LOGO"
         android:layout_width="193dp"
         android:layout_height="168dp"
         android:layout_marginStart="110dp"
         android:layout_marginTop="180dp"
         android:layout_marginEnd="110dp"
         android:layout_marginBottom="100dp"
         android:background="@drawable/ic_logo_topics"
         android:cropToPadding="false"
         app:layout_constraintBottom_toTopOf="@+id/botonRegistro"
         app:layout_constraintEnd_toEndOf="parent"
         app:layout_constraintStart_toStartOf="parent"
         app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.gms.common.SignInButton
         android:id="@+id/botonGoogle"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_marginStart="85dp"
         android:layout_marginTop="600dp"
         android:background="#00FFFFFF"
         android:src="@drawable/ic_boton_google"
         app:layout_constraintStart_toStartOf="parent"
         app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.gms.common.SignInButton
         android:id="@+id/boton_Twitter"
         android:layout_width="65dp"
         android:layout_height="61dp"
         android:layout_marginStart="15dp"
         android:layout_marginTop="603dp"
         android:layout_marginEnd="20dp"
         android:background="#00FFFFFF"
         android:src="@drawable/ic_boton_twitter"
         app:layout_constraintEnd_toStartOf="@+id/botonFacebook"
         app:layout_constraintStart_toEndOf="@+id/botonGoogle"
         app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
         android:id="@+id/botonFacebook"
         android:layout_width="59dp"
         android:layout_height="58dp"
         android:layout_marginTop="604dp"
         android:layout_marginEnd="78dp"
         android:background="#00FFFFFF"
         android:src="@drawable/ic_boton_facebook"
         app:layout_constraintEnd_toEndOf="parent"
         app:layout_constraintTop_toTopOf="parent" />

    <TextView
         android:id="@+id/textView5"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_marginBottom="30dp"
         android:text="Al continuar acepta los tÃ©rminos y condicones de uso de datos."
         android:textSize="12dp"
         app:layout_constraintBottom_toBottomOf="parent"
         app:layout_constraintEnd_toEndOf="parent"
         app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>